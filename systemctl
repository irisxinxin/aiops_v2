[Service]
WorkingDirectory=/home/ubuntu/huixin/aiops_v2
Type=simple
ExecStart=
ExecStart=/usr/bin/bash -lc 'bash scripts/start_qproxy.sh'
Restart=always
RestartSec=3
MemoryMax=1G
CPUQuota=50%
TasksMax=512
Environment=PYTHONUNBUFFERED=1
Environment=Q_CMD=/home/ubuntu/.local/bin/q
Environment=POOL_SIZE=1
Environment=READY_NEED=0
Environment=WARMUP_DELAY_MS=1000
Environment=QPROXY_LOG_LEVEL=INFO
Environment=HTTP_HOST=0.0.0.0
Environment=HTTP_PORT=8080
