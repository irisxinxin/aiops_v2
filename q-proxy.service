[Unit]
Description=Q HTTP Proxy Service
After=network.target

[Service]
Type=exec
User=ubuntu
Group=ubuntu
WorkingDirectory=/home/ubuntu/huixin/aiops_v2
ExecStart=/usr/bin/python3 /home/ubuntu/huixin/aiops_v2/http_proxy.py
Restart=always
RestartSec=5
StandardOutput=journal
StandardError=journal

[Install]
WantedBy=multi-user.target
